<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="address_custom" model="ir.ui.view">
        <field name="name">address_custom</field>
        <field name="inherit_id" search="[('key', '=', 'website_sale.cart_summary')]"/>
        <field name="type">qweb</field>
        <field name="key">web_add_sku_multip_page.address_custom</field>
        <field name="arch" type="xml">
            <xpath expr="//table[@id='cart_products']//tbody//td[@class='td-product_name']//div" position="inside">
                <br/>
                <span>
                    <strong>SKU:</strong>
                    <strong t-field="line.product_id.default_code"/>
                </span>
            </xpath>
        </field>
    </record>
</odoo>